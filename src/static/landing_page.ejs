<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="landing_page.css" />
    <title>AutoBot</title>
  </head>
  <body>
    <div class="container">
      <section id="content">
        <main>
          <header>
            <h1>AutoBot</h1>
          </header>
          <button id="start-btn">Start bot</button>
          <button id="restart-btn">restart</button>
          <button id="upload-books">Upload books</button>
          <input
            type="text"
            id="sheet-title"
            placeholder="Enter sheet title"
            class="input"
          />
          <input
            class="input"
            type="text"
            id="spreadsheet-id"
            placeholder="Enter spreadsheet id"
          />
        </main>
        <aside></aside>
        <aside id="console"></aside>
      </section>
    </div>
    <script src="socket.io/socket.io.js"></script>
    <script src="landing_page.js"></script>
  </body>
</html>
